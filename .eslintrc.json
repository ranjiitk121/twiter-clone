{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes": ["error", "single", { "avoidEscape": true }],
        "no-trailing-spaces": "error",
        "no-console": "error",
        "keyword-spacing": "error",
        "semi": ["error", "always"]
    }
}
